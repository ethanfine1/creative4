n\n<style>\n  .intro {\n    font-size: 1.7em;\n    margin-right: 100px;\n    margin-left: 100px;\n  }\n  h1 {\n    font-size: 2.5em;\n  }\n  input {\n    font-size: 1.2em;\n    height: 30px;\n    width: 200px;\n  }\n  textarea {\n    font-size: 1.6em;\n    width: 100%;\n    max-width: 500px;\n    height: 100px;\n  }\n</style>\n\n<script>\nexport default {\n  name: 'create',\n  data() {\n    return {\n      creating: true,\n      title: '',\n      author: '',\n      reasoning: '',\n    }\n  },\n  methods: {\n    toggleForm() {\n      this.creating = !this.creating;\n    },\n    addSuggestion() {\n      this.$root.$data.addSuggestions(this.title, this.author, this.reasoning);\n      this.name = \"\";\n      this.problem = \"\";\n      this.creating = false;\n    },\n  }\n}\n</script>\n"]}]}